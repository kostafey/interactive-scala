{
	"name": "interactive-scala",
	"displayName": "Interactive Scala",
	"description": "Run Scala code by a click on a button!",
	"publisher": "alssys",
	"version": "0.1.0",
	"repository": "https://github.com/matt035343/interactive-scala",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:scala"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.executeInInteractiveScala",
				"title": "Exectute selected code in Interactive Scala"
			}
		],
		"keybindings": [
			{
				"command": "extension.executeInInteractiveScala",
				"key": "alt+enter",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
